    "pre_install": [
        ". \"$bucketsdir\\$bucket\\bin\\utils.ps1\"",
        "& \"$dir\\XBYDriver-$($version)-win-x64.exe\" /S /D=$dir",
        "persist -dir @(\"$env:AppData\\alixby3\") @(\"$persist_dir\\alixby3\")",
        "create_app_lnk \"$dir\\小白羊云盘.exe\" \"$scoop_apps_lnk\\小白羊云盘.lnk\"",
        "clean_redundant_files @(\"C:\\ProgramData\\Microsoft\\Windows\\Start Menu\\Programs\\小白羊云盘.lnk\",\"$dir\\XBYDriver-$($version)-win-x64.exe\") 10"
    ],
    "pre_uninstall": [
        "$ErrorActionPreference=\"SilentlyContinue\"",
        "& \"$dir\\Uninstall 小白羊云盘.exe\" /S",
        "$bucket=(scoop bucket list | where-Object { $_.Source -match '(gitee|github).com/abgox/abgo_bucket'})[0].Name",
        ". \"$bucketsdir\\$bucket\\bin\\utils.ps1\"",
        "stop_process",
        "remove_files @(\"C:\\Users\\Public\\Desktop\\小白羊云盘.lnk\",\"$scoop_apps_lnk\\小白羊云盘.lnk\",\"$env:AppData\\alixby3\")"
    ],
    "checkver": "github",
