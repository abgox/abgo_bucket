    "pre_install": [
        "$bucket=(scoop bucket list | where-Object { $_.Source -match '(gitee|github).com/abgox/abgo_bucket'})[0].Name",
        ". \"$bucketsdir\\$bucket\\bin\\utils.ps1\"",
        "persist -dir @(\"$user_AppData\\StrokesPlus.net\") @(\"$persist_dir\\StrokesPlus.net\")",
        "create_app_lnk \"$dir\\StrokesPlus.net.exe\" \"$desktop\\StrokesPlus.net.lnk\""
    ],
    "pre_uninstall": [
        "$bucket=(scoop bucket list | where-Object { $_.Source -match '(gitee|github).com/abgox/abgo_bucket'})[0].Name",
        ". \"$bucketsdir\\$bucket\\bin\\utils.ps1\"",
        "stop_process",
        "remove_files @(\"$desktop\\StrokesPlus.net.lnk\",\"$user_AppData\\StrokesPlus.net\")"
    ],
    "checkver": {
        "url": "https://www.strokesplus.net/Downloads",
        "regex": "Latest Release\\: <b>([\\d\\.]+)"
