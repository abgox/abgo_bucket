        "stop_process",
        "remove_files @(\"$desktop\\阿里云盘.lnk\",\"$user_AppData\\aDrive\")"
    ],
    "checkver": {
        "url": "https://www.aliyundrive.com/desktop/version/update.json",
        "regex": "/([\\d\\.]+)"
