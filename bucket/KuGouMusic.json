        ". \"$bucketsdir\\$bucket\\bin\\utils.ps1\"",
        "stop_process $false",
        "remove_files @(\"$user_AppData\\KuGou8\")",
        "$ui_exe=\"$dir\\uninst.exe\"",
        "try{ & $ui_exe /S }catch{}",
        "sleep_uninstall $ui_exe",
        "remove_file \"$user_AppData\\KuGou8\""
