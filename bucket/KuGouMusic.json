        "$core_exe=\"$dir\\KuGou.exe\"",
        "sleep_install $core_exe",
        "persist -dir @(\"$user_AppData\\KuGou8\") @(\"$persist_dir\\KuGou8\")",
        "clean_redundant_files @($path_exe)",
        "remove_app_lnk @(\"酷狗音乐.lnk\") @(\"酷狗音乐\")"
    ],
    "pre_uninstall": [
