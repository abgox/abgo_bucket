    "post_install": [
        ". \"$bucketsdir\\$bucket\\bin\\utils.ps1\"",
        "try{Invoke-Expression $dir\\install-pyenv-win.ps1}catch{throw 'Install error due to possible network issues.'}",
        "persist -dir @(\"$env:userProfile\\.pyenv\") @(\"$persist_dir\\.pyenv\")"
    ],
    "pre_uninstall": [
        "Invoke-Expression \"$dir\\install-pyenv-win.ps1 -Uninstall\""
    ],
    "checkver": "github",
    "autoupdate": {
