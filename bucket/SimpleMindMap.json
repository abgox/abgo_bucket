{
    "version": "0.9.10",
    "description": "一个相对强大的Web思维导图。A relatively powerful web mind map.",
    "homepage": "https://wanglin2.github.io/mind-map/#/index",
    "license": {
        "identifier": "MIT",
        "url": "https://github.com/wanglin2/mind-map/blob/main/LICENSE"
    },
    "url": "https://github.com/wanglin2/mind-map/releases/download/electron-0.9.10/Setup.0.9.10.exe#/_installer_.exe",
    "hash": "34902dbd6b97d60fefa26f4575a08e42149cf08e7f6b80731c1dc12ffcfa9032",
    "shortcuts": [
        [
            "思绪思维导图.exe",
            "思绪思维导图"
        ]
    ],
    "pre_install": [
        ". \"$bucketsdir\\$bucket\\bin\\utils.ps1\"",
        "$path_exe=\"$dir\\_installer_.exe\"",
        "& $path_exe /S /D=$dir",
        "$core_exe=\"$dir\\思绪思维导图.exe\"",
        "sleep_install $core_exe",
        "persist -dir @(\"$user_AppData\\thoughts\") @(\"$persist_dir\\thoughts\")",
        "clean_redundant_files @($path_exe,\"$admin_apps_lnk\\思绪思维导图.lnk\")",
        "remove_app_lnk @(\"思绪思维导图.lnk\")"
    ],
    "pre_uninstall": [
        "$bucket=(scoop bucket list | where-Object { $_.Source -match '(gitee|github).com/abgox/abgo_bucket'})[0].Name",
        ". \"$bucketsdir\\$bucket\\bin\\utils.ps1\"",
        "stop_process $false",
        "remove_files @(\"$user_AppData\\thoughts\")",
        "$ui_exe=\"$dir\\Uninstall 思绪思维导图.exe\"",
        "try{ & $ui_exe /S }catch{}",
        "sleep_uninstall $ui_exe",
        "remove_files @(\"$user_LocalAppData\\thoughts-updater\")"
    ],
    "checkver": {
        "github": "https://github.com/wanglin2/mind-map",
        "regex": "electron-([\\d\\.]+)"
    },
    "autoupdate": {
        "url": "https://github.com/wanglin2/mind-map/releases/download/electron-$version/Setup.$version.exe#/_installer_.exe"
    }
}
