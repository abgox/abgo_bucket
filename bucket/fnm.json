    "hash": "f083e7256c24f359d2101934426c87e3066831a40303ebe6e538cea917a0f184",
    "bin": "fnm.exe",
    "post_install": [
        ". \"$bucketsdir\\$bucket\\bin\\utils.ps1\"",
        "handle_lang -CN {",
        "   write_with_color \"`n<@Yellow>不要忘记将 <@Magenta>fnm env --use-on-cd | Out-String | Invoke-Expression<@Yellow> 添加到你的 powershell 配置文件中`n\"",
        "} -EN {",
        "   write_with_color \"`n<@Yellow>Don't forget to add <@Magenta>fnm env --use-on-cd | Out-String | Invoke-Expression<@Yellow> to your powershell profile.`n\"",
        "}"
    ],
    "pre_uninstall": [
        "$bucket=(scoop bucket list | where-Object { $_.Source -match '(gitee|github).com/abgox/abgo_bucket'})[0].Name",
        ". \"$bucketsdir\\$bucket\\bin\\utils.ps1\"",
        "stop_process"
    ],
    "checkver": "github",
    "autoupdate": {
        "url": "https://github.com/Schniz/fnm/releases/download/v$version/fnm-windows.zip"
    }
